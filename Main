package com.gmail.bizkit245;

import java.util.LinkedList;
import java.util.List;

public class Main {

	public static void main(String[] args) {

		List<Integer> list = new LinkedList<>();
		for (int i = 0; i < 10; i++) {
			list.add(i);
			System.out.print(list.get(i) + " ");
		}

		System.out.println();

		list.remove(0);
		list.remove(0);
		list.remove(list.size() - 1);
		
		for (Integer i : list) {
			System.out.print(i + " ");
		}
	}

}
